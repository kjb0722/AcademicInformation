--관리자
CREATE TABLE ADMIN
(
    NUM NUMBER CONSTRAINT ADMIN_NUM_PK PRIMARY KEY,
    ID VARCHAR(30),
    PW VARCHAR(30)
);

CREATE SEQUENCE ADMIN_NUM_SEQ;

--과목
CREATE TABLE SUBJECT
(

);

--교수
CREATE TABLE PROFESSOR
(
    NUM NUMBER CONSTRAINT PROFESSOR_NUN_PK PRIMARY KEY,
    ID VARCHAR(30),
    PW VARCHAR(30),
    EMAIL VARCHAR(40),
    PHONE VARCHAR(30)
);